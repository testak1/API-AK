{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "studio/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/studio/(.*)",
      "dest": "/studio/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
