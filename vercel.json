{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "studio/package.json",
      "use": "@vercel/static-build"
    },

    {
      "src": "api/brands.ts",
      "use": "@vercel/node"
    }


  ],
  "routes": [
    {
      "src": "/api/brands",
      "dest": "/api/brands.ts"
    },
    {
      "src": "studio/(.*)",
      "dest": "/studio/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
